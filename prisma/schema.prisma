generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model admin_notification {
  id              Int       @id @default(autoincrement())
  title           String?   @db.VarChar(255)
  description     String?   @db.Text
  country         String?   @db.VarChar(45)
  status          Int?      @default(0) @db.TinyInt
  url             String?   @db.VarChar(255)
  created_at      DateTime? @default(now()) @db.Timestamp(0)
  updated_at      DateTime? @default(now()) @db.Timestamp(0)
  icon            String?   @db.VarChar(255)
  redirect_type   String?   @db.VarChar(255)
  ask_again       Int?      @db.TinyInt
  repeat_duration Int?
  button_cancel   String?   @db.VarChar(50)
  button_oke      String?   @db.VarChar(50)
  popup_type      String?   @default("popup") @db.VarChar(45)
  toasts_color    String?   @db.VarChar(45)
}

model batchs {

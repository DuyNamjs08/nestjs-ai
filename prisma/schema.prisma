model coupon_codes {
  id                Int       @id @default(autoincrement())
  coupon_id         Int?
  code              String    @unique(map: "code_UNIQUE") @db.VarChar(20)
  is_used           Int       @default(0) @db.TinyInt
  is_active         Int?      @default(0) @db.TinyInt
  used_at           DateTime? @db.DateTime(0)
  used_by           Int?
  expired_at        DateTime? @db.DateTime(0)
  value             Int?      @default(30)
  num_days          Int?      @default(30)
  created_at        DateTime? @db.DateTime(0)
  updated_at        DateTime? @db.DateTime(0)
  total_link_active Int?      @default(7)
  coupons           coupons?  @relation(fields: [coupon_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "coupon_code_coupon_id")

  @@index([coupon_id], map: "coupon_code_coupon_id_idx")
  @@index([used_by], map: "coupon_code_used_by")
}

